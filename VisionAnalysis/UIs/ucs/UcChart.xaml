<UserControl x:Class="VisionAnalysis.UcChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VisionAnalysis"
             xmlns:formDV="clr-namespace:System.Windows.Forms.DataVisualization.Charting;assembly=System.Windows.Forms.DataVisualization"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <WindowsFormsHost>
            <formDV:Chart x:Name="chart1">
                <formDV:Chart.ChartAreas>
                    <formDV:ChartArea/>
                </formDV:Chart.ChartAreas>

            </formDV:Chart>
        </WindowsFormsHost>
    </Grid>
</UserControl>
