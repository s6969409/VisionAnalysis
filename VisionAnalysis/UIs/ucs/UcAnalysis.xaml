<UserControl x:Class="VisionAnalysis.UcAnalysis"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VisionAnalysis"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TabControl>
        <TabItem Header="Image">
            <local:UcImage x:Name="img"/>
        </TabItem>
        <TabItem Header="Table" x:Name="ti_table">
            <local:UcTable x:Name="table"/>
        </TabItem>
        <TabItem Header="Object">
            <local:UcTree x:Name="tv_obj"/>
        </TabItem>
    </TabControl>
</UserControl>
